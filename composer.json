        "catcher-cloud/catcher-php": "@dev"
    },
    "autoload": {
        "psr-4": {
            "Catcher\\Laravel\\": "src/"
        }
    },
    "require-dev": {
        "laravel/laravel": "^9.3"
    },
    "extra": {
